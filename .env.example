# Cal.com Configuration
CALCOM_API_KEY=cal_xxxx_xxxxxxxxxxxxxxxx             # Your Cal.com API key
CALCOM_EVENT_TYPE_ID=123456                          # The numeric ID of your event type
CALCOM_EVENT_DURATION=30                             # Duration of the event in minutes
CALCOM_USERNAME=your-username                        # Your Cal.com username
CALCOM_EVENT_SLUG=meeting                            # The slug of your event type (from cal.com/username/[slug])
CALCOM_BOOKING_LINK=https://cal.com/${CALCOM_USERNAME}/${CALCOM_EVENT_SLUG}       # Your booking link for demos

# Database Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key          # Used for client-side authentication
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key      # Used for server-side operations (e.g., cron)

# VAPI Configuration
VAPI_API_KEY=vapi_xxxx_xxxxxxxxxxxxxxxx
VAPI_ASSISTANT_ID=asst_xxxx_xxxxxxxxxxxxxxxx
VAPI_PHONE_NUMBER_ID=phn_xxxx_xxxxxxxxxxxxxxxx

# VAPI Integration Webhook
VAPI_SECRET_KEY=your_generated_webhook_secret_here   # Secret for authenticating VAPI webhooks to our endpoints
AI_DIALER_URL=https://your-domain.com                # Base URL for your AI dialer application

# Cron Configuration
CRON_SECRET=your-secret-here                         # Secret for authenticating cron job requests

# Email Configuration (Resend)
RESEND_API_KEY=re_xxxx_xxxxxxxxxxxxxxxx
RESEND_FROM_EMAIL=team@example.com                   # The email address to send from
RESEND_FROM_NAME=AI Dialer Team                      # The name to show in the from field

# Node Environment
NODE_ENV=development
